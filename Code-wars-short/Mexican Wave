#include <vector>
#include <string>
#include <cctype>

std::vector<std::string> wave(std::string y) {
    std::vector<std::string> result;
    int len = y.size();
    
    for (int i = 0; i < len; i++) {
        if (std::isalpha(y[i])) { // Check if it's an alphabet character
            y[i] = std::toupper(y[i]); // Convert the character to uppercase
            result.push_back(y); // Add the modified string to the result vector
            y[i] = std::tolower(y[i]); // Convert the character back to lowercase
        }
    }

    return result; // Return the result vector with the Mexican Wave
}
